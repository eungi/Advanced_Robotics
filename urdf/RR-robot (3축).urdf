<?xml version="1.0" ?>
<robot name="RR-robot">

  <material name="black">
    <color rgba="0.0 0.0 0.0 1.0"/>
  </material>
  <material name="orange">
    <color rgba="1.0 0.4 0.0 1.0"/>
  </material>
  <material name="blue">
    <color rgba="0.0 0.0 1.0 1.0"/>
  </material>
  <material name="green">
    <color rgba="0.0 1.0 0.0 1.0"/>
  </material>
  <material name="red">
    <color rgba="1.0 0.0 0.0 1.0"/>
  </material>

  <link name="base">
  </link>

  <joint name="joint0" type="revolute">
    <parent link="base"/>
    <child link="link1"/>
    <origin xyz="0 0 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="30" lower="-2.617" upper="2.617" velocity="1.571"/>
  </joint>

  <link name="link1">
    <collision>
      <origin xyz="0 0 0" rpy="1.571 0 1.571"/>
      <geometry>
      	<mesh filename="package://robot/meshes/chain_link3.stl" scale="0.0075 0.0075 0.0075"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="1.571 0 1.571"/>
      <geometry>
      	<mesh filename="package://robot/meshes/chain_link3.stl" scale="0.0075 0.0075 0.0075"/>
      </geometry>
      <material name="orange"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <joint name="joint1" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0.957 0.186 0" rpy="0 0 0"/>
    <axis xyz="0 0 1"/>
    <limit effort="30" lower="-2.617" upper="2.617" velocity="1.571"/>
  </joint>

  <link name="link2">
    <collision>
      <origin xyz="0 0 0" rpy="1.571 0 0"/>
      <geometry>
      	<mesh filename="package://robot/meshes/chain_link4.stl" scale="0.0075 0.0075 0.0075"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="1.571 0 0"/>
      <geometry>
      	<mesh filename="package://robot/meshes/chain_link4.stl" scale="0.0075 0.0075 0.0075"/>
      </geometry>
      <material name="blue"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <joint name="joint2" type="fixed">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0.93 0.0 0.0" rpy="0 0 0"/>
  </joint>

  <link name="link3">
    <collision>
      <origin xyz="0 0 0" rpy="1.571 0 0"/>
      <geometry>
      	<mesh filename="package://robot/meshes/chain_link5.stl" scale="0.0075 0.0075 0.0075"/>
      </geometry>
    </collision>
    <visual>
      <origin xyz="0 0 0" rpy="1.571 0 0"/>
      <geometry>
      	<mesh filename="package://robot/meshes/chain_link5.stl" scale="0.0075 0.0075 0.0075"/>
      </geometry>
      <material name="green"/>
    </visual>
    <inertial>
      <origin xyz="0 0 0" rpy="0 0 0"/>
      <mass value="1"/>
      <inertia ixx="1.0" ixy="0.0" ixz="0.0" iyy="1.0" iyz="0.0" izz="1.0"/>
    </inertial>
  </link>

  <!--  Grip link -->
  <link name="grip_link">
    <visual>
      <origin xyz="0.028 0.0246 0" rpy="1.571 0 0"/>
      <geometry>
        <mesh filename="package://robot/meshes/chain_link_grip_l.stl" scale="0.0075 0.0075 0.0075"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0.028 0.0246 0" rpy="1.571 0 0"/>
      <geometry>
        <mesh filename="package://robot/meshes/chain_link_grip_l.stl" scale="0.0075 0.0075 0.0075"/>
      </geometry>
      <material name="red"/>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.017" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0"
               iyy="0.1" iyz="0.0"
               izz="0.1" />
    </inertial>
  </link>

  <!--  Grip joint -->
  <joint name="grip_joint" type="fixed">
    <parent link="link3"/>
    <child link="grip_link"/>
    <origin xyz="0.5136 -0.025 0.2" rpy="0 0 0"/>
  </joint>

  <!--  Grip link sub -->
  <link name="grip_link_sub">
    <visual>
      <origin xyz="0.028 -0.0246 0" rpy="1.571 0 0"/>
      <geometry>
        <mesh filename="package://robot/meshes/chain_link_grip_r.stl" scale="0.0075 0.0075 0.0075"/>
      </geometry>
    </visual>

    <collision>
      <origin xyz="0.028 -0.0246 0" rpy="1.571 0 0"/>
      <geometry>
        <mesh filename="package://robot/meshes/chain_link_grip_r.stl" scale="0.0075 0.0075 0.0075"/>
      </geometry>
      <material name="red"/>
    </collision>

    <inertial>
      <origin xyz="0 0 0" />
      <mass value="0.017" />
      <inertia ixx="0.1" ixy="0.0" ixz="0.0"
               iyy="0.1" iyz="0.0"
               izz="0.1" />
    </inertial>
  </link>

  <!--  Grip joint sub -->
  <joint name="grip_joint_sub" type="fixed">
    <parent link="link3"/>
    <child link="grip_link_sub"/>
    <origin xyz="0.5136 0.025 -0.2" rpy="0 0 0"/>
    <axis xyz="0 1 0"/>
    <limit velocity="3.5" effort="100" lower="-0.015" upper="0.0105" />
  </joint>

</robot>
